{
  "name": "ultimateprogressbar",
  "version": "v0.0.1-beta.1",
  "description": "indev game idea",
  "main": "./dist/bundle.js",
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "husky": "^7.0.4",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "localforage": "^1.10.0"
  },
  "scripts": {
    "lint": "npx eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "csslint": "npx stylelint styles.css",
    "csslint:fix": "npx stylelint styles.css --fix",
    "build:esbuild": "npx esbuild src/Game.ts --bundle --minify --sourcemap --target=\"chrome58,firefox57,safari11,edge29\" --outfile=\"./dist/out.js\"",
    "watch:esbuild": "npx esbuild src/Game.ts --bundle --minify --sourcemap --target=\"chrome58,firefox57,safari11,edge29\" --outfile=\"./dist/out.js\" --watch",
    "check:tsc": "npx -p typescript tsc",
    "watch:tsc": "npx -p typescript tsc --watch",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pseudo-Corp/Ultra-Progress-Bar.git"
  },
  "keywords": [
    "Ultimate Progress Bar",
    "Ultimate",
    "Progress",
    "Bar",
    "Platonic"
  ],
  "author": "Kevin Bunn",
  "license": "MIT",
  "homepage": "https://github.com/Pseudo-Corp/Ultra-Progress-Bar#readme"
}
